@{
    foreach (var item in Model)
    {
        <li class="list-group-item" style="padding: 10px 0px;">
            <a href="@Url.Action("SachTheoCD", "Book", new { @maCD = @item.MaLoai })" style="text-transform:uppercase;text-decoration:none ;
